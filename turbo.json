{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "prepublishOnly": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "artifacts:download": {
      "outputs": [],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_USERNAME",
    "DATABASE_PASSWORD",
    "DATABASE_HOST",
    "DATABASE_DB_NAME",
    "DATABASE_SSL",
    "PASSPORT_SERVER_URL",
    "NODE_ENV",
    "PORT",
    "HONEYCOMB_API_KEY",
    "MAILGUN_API_KEY",
    "TIMING",
    "BYPASS_EMAIL_REGISTRATION",
    "ROLLBAR_TOKEN",
    "SW_ID",
    "IS_ZUZALU",
    "SUPPRESS_LOGGING",
    "ROLLBAR_ENV_NAME",
    "SERVER_RSA_PRIVATE_KEY_BASE64",
    "DISCORD_TOKEN",
    "DISCORD_ALERTS_CHANNEL_ID",
    "SERVER_EDDSA_PRIVATE_KEY",
    "TELEGRAM_BOT_TOKEN",
    "TELEGRAM_BOT_START_DELAY_MS",
    "PASSPORT_CLIENT_URL",
    "PRETIX_SYNC_DISABLED",
    "IS_LOCAL_HTTPS",
    "PRETIX_TOKEN",
    "PRETIX_ORG_URL"
  ]
}
