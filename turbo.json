{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".nvmrc"],
      "outputs": [],
      "cache": true
    },
    "lint": {
      "dependsOn": ["build"],
      "cache": true
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "clean-test-artifacts": {
      "cache": false
    },
    "gen-test-artifacts": {
      "dependsOn": ["^gen-test-artifacts"],
      "cache": true
    },
    "test": {
      "dependsOn": ["^build", "^gen-test-artifacts"],
      "inputs": ["$TURBO_DEFAULT$", ".mocharc.js"],
      "cache": true
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "prepublishOnly": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "artifacts:download": {
      "outputs": [],
      "cache": false
    },
    "//#test-typedoc": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "typedoc.json"],
      "outputs": [],
      "cache": true
    },
    "//#test-generated-package": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "scripts/testTemplatePackage.ts"],
      "outputs": [],
      "cache": true
    }
  },
