{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "c/**", "js/**", "calcwit.js", "main.js", "rollup.cjs.config.js"],
      "outputs": ["build/**"],
      "cache": true
    },
    "test": {
      "cache": true,
      "dependsOn": ["build"],
      "inputs": ["$TURBO_DEFAULT$"]
    }
  }
}
