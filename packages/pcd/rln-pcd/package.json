{
  "name": "@pcd/rln-pcd",
  "version": "0.13.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./RLNPCD": {
      "types": "./dist/types/src/RLNPCD.d.ts",
      "import": "./dist/esm/src/RLNPCD.js",
      "require": "./dist/cjs/src/RLNPCD.js"
    }
  },
  "files": [
    "./artifacts",
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "test": "ts-mocha --type-check --config ../../../.mocharc.js --exit 'test/**/*.spec.ts'",
    "build": "tsc -b tsconfig.cjs.json tsconfig.esm.json",
    "postbuild": "fix-esm-import-path ./dist/esm/src",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf dist node_modules *.tsbuildinfo"
  },
  "dependencies": {
    "@pcd/pcd-types": "^0.14.0",
    "@pcd/semaphore-group-pcd": "^0.14.0",
    "@pcd/semaphore-identity-pcd": "^0.14.0",
    "@pcd/util": "0.8.0",
    "@semaphore-protocol/group": "^3.15.2",
    "@semaphore-protocol/identity": "^3.15.2",
    "json-bigint": "^1.0.0",
    "rlnjs": "^2.0.8",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "0.14.0",
    "@pcd/tsconfig": "0.14.0",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "eslint": "^8.57.0",
    "fix-esm-import-path": "^1.10.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
