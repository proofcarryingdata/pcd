{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "src/**",
        "test/**",
        "typedoc.json",
        "artifacts/**",
        "circuits/**"
      ],
      "outputs": ["dist/**", "*.tsbuildinfo"],
      "cache": true
    },
    "test": {
      "cache": true,
      "dependsOn": ["build"],
      "inputs": ["$TURBO_DEFAULT$", "test/**"]
    }
  }
}
